# Soprano Voice Pipeline API Keys
# Copy this file to .env and add your actual API keys

# OpenAI Speech-to-Text (Whisper)
EXPO_PUBLIC_OPENAI_API_KEY=your_openai_api_key_here

# Anthropic Claude API
EXPO_PUBLIC_ANTHROPIC_API_KEY=your_anthropic_api_key_here

# ElevenLabs Text-to-Speech
EXPO_PUBLIC_ELEVENLABS_API_KEY=your_elevenlabs_api_key_here
EXPO_PUBLIC_ELEVENLABS_VOICE_ID=your_default_voice_id_here

# Optional: Language-specific voice IDs for multilingual support
# If not set, ELEVENLABS_VOICE_ID will be used as fallback
EXPO_PUBLIC_ELEVENLABS_VOICE_ID_ENGLISH=your_english_voice_id_here
EXPO_PUBLIC_ELEVENLABS_VOICE_ID_HINDI=your_hindi_voice_id_here
